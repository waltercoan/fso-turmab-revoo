@startuml
skinparam classAttributeIconSize 0
' left to right direction
caption Diagrama de entidades
namespace entidades {
    class Cliente {
        - String nome
        - String endereco
        + Cliente(nome:String)
        + getNome():String
        + setNome(nome:String):void
        + getEndereco():String
        + setEndereco(endereco:String ):void
        + getCliente():Cliente
        + setCliente(cliente:Cliente):void
        + toString():String
    }
    class Cidade {
        - String nome
        + Cidade(nome:String)
    }
    class Object {
        + toString():String
    }
    Cliente "-cidade *" -> "1" Cidade
    Cliente --|>  Object
}
@enduml